<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <EditTextPreference
        android:title="@string/pref_result_title"
        android:key="@string/pref_result_key"
        android:summary="@string/pref_notification_summary">
    </EditTextPreference>
    <CheckBoxPreference
        android:title="@string/pref_notification_title"
        android:defaultValue="@string/pref_notification_default"
        android:summary="@string/pref_notification_summary"
        android:key="@string/pref_notification_key" />
    <CheckBoxPreference
        android:title="@string/pref_location_title"
        android:defaultValue="@string/pref_location_default"
        android:summary="@string/pref_location_summary"
        android:key="@string/pref_location_key" />
    <CheckBoxPreference
        android:title="@string/pref_GPS_title"
        android:defaultValue="@string/pref_GPS_default"
        android:summary="@string/pref_GPS_summary"
        android:key="@string/pref_GPS_key" />

</PreferenceScreen>
